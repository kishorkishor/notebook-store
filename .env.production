# This file contains environment variables for production
# (Note: In actual deployment, set these in Netlify UI instead of this file)

NEXTAUTH_URL=https://notebook-store.netlify.app
NEXTAUTH_SECRET=your_nextauth_secret_should_be_set_in_netlify_ui

# Database URL placeholder - replace with actual value in Netlify UI
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/notebooks

# Set NODE_ENV explicitly
NODE_ENV=production

# Netlify specific variables
NEXT_USE_NETLIFY_EDGE=true
NEXT_FORCE_EDGE_IMAGES=true 